{
  "name": "apidoc-client",
  "version": "1.0.0",
  "description": "Client app for Api documents",
  "scripts": {
    "stats": "webpack --profile --json > stats.json",
    "start": "webpack-dev-server",
    "deploy": "gh-pages -d build",
    "build": "webpack",
    "test": "mocha --compilers js:babel-core/register --require ignore-styles ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch",
    "test:tdd": "npm run test -- --auto-watch --no-single-run",
    "test:lint": "eslint ./app ./tests --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache"
  },
  "author": "Beatusvir",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "3.5.0",
    "chai-immutable": "1.5.3",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "eslint": "2.10.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.17.0",
    "html-webpack-template": "^5.0.0",
    "ignore-styles": "^4.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "jsdom": "8.0.4",
    "mocha": "^2.5.2",
    "node-sass": "^3.8.0",
    "npm-install-webpack-plugin": "4.0.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.13.0",
    "webpack-validator": "^2.1.1"
  },
  "dependencies": {
    "autoprefixer": "6.3.3",
    "classnames": "2.2.3",
    "immutable": "3.7.6",
    "object-assign": "4.0.1",
    "react": "^15.1.0",
    "react-addons-perf": "^15.1.0",
    "react-addons-pure-render-mixin": "0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^15.1.0",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "redux": "3.3.1",
    "socket.io-client": "1.4.5",
    "uuid": "2.0.1"
  }
}
